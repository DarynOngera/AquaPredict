# Google Earth Engine
GEE_SERVICE_ACCOUNT=your-service-account@project.iam.gserviceaccount.com
GEE_PRIVATE_KEY_FILE=./credentials/gee_key.json

# Oracle Database
DATABASE_URL=oracle+oracledb://username:password@hostname:1521/?service_name=servicename
DB_USER=aquapredict
DB_PASSWORD=your_secure_password
DB_HOST=adb.region.oraclecloud.com
DB_PORT=1521
DB_SERVICE=aquapredict_high

# OCI Configuration
OCI_TENANCY_OCID=ocid1.tenancy.oc1..xxx
OCI_USER_OCID=ocid1.user.oc1..xxx
OCI_FINGERPRINT=xx:xx:xx:xx:xx:xx:xx:xx:xx:xx:xx:xx:xx:xx:xx:xx
OCI_PRIVATE_KEY_PATH=./credentials/oci_api_key.pem
OCI_REGION=us-ashburn-1
OCI_COMPARTMENT_OCID=ocid1.compartment.oc1..xxx

# Object Storage
OCI_BUCKET_NAME=aquapredict-data
OCI_NAMESPACE=your-namespace

# Model Deployment
MODEL_DEPLOYMENT_ENDPOINT=https://modeldeployment.region.oci.oraclecloud.com/ocid1.datasciencemodeldeployment.oc1..xxx

# Mapbox (for frontend maps)
MAPBOX_TOKEN=pk.your_mapbox_token

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
API_WORKERS=4

# Airflow
AIRFLOW_UID=50000
AIRFLOW__CORE__EXECUTOR=CeleryExecutor
AIRFLOW__CORE__SQL_ALCHEMY_CONN=postgresql+psycopg2://airflow:airflow@postgres/airflow

# Application Settings
ENVIRONMENT=development
LOG_LEVEL=INFO
DEBUG=True

# Kenya Pilot Configuration
KENYA_BOUNDS_WEST=33.9
KENYA_BOUNDS_SOUTH=-4.7
KENYA_BOUNDS_EAST=41.9
KENYA_BOUNDS_NORTH=5.5
GRID_RESOLUTION_KM=1

# Model Training
TRAIN_TEST_SPLIT=0.8
RANDOM_SEED=42
CV_FOLDS=5
